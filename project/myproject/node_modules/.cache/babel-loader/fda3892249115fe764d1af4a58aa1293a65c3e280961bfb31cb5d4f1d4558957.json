:Footer\r\n        },\r\n        data(){\r\n             return {\r\n                List:[\r\n                  \r\n                ]\r\n            }\r\n        },\r\n        methods:{\r\n            add_item(item){\r\n                this.List.unshift(item)\r\n            },\r\n            change_item(id){\r\n                    this.List.forEach((item)=>{\r\n                        if(item.id===id) item.done=!item.done\r\n                    })\r\n            },\r\n            delete_item(id){\r\n                console.log(id)\r\n                this.List=this.List.filter((item)=>{\r\n                   return item.id!==id\r\n                })\r\n            },\r\n            checkall(flag){\r\n                this.List.forEach((item)=>{\r\n                    item.done=flag\r\n                })\r\n            },\r\n            cleardoneitem(){\r\n               this.List= this.List.filter((item)=>{\r\n                    return !item.done\r\n                })\r\n            }\r\n        },\r\n        watch:{\r\n            List(value){\r\n                localStorage.setItem('List',JSON.stringify(value))\r\n            }\r\n        }\r\n            \r\n        }\r\n    \r\n</script>\r\n\r\n<style>\r\n    #outer{\r\n        border: 1px solid black;\r\n        width: 530px;\r\n        padding-left:10px ;\r\n        padding-top:10px;\r\n        padding-bottom: 10px;\r\n        position: absolute;\r\n    }\r\n</style>\r\n"],"mappings":";AAUA,OAAAA,GAAA;AACA,OAAAC,IAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,GAAA,EAAAA,GAAA;IACAC,IAAA,EAAAA,IAAA;IACAC,MAAA,EAAAA;EACA;EACAG,KAAA;IACA;MACAJ,IAAA;IAGA;EACA;EACAK,OAAA;IACAC,SAAAC,IAAA;MACA,KAAAP,IAAA,CAAAQ,OAAA,CAAAD,IAAA;IACA;IACAE,YAAAC,EAAA;MACA,KAAAV,IAAA,CAAAW,OAAA,CAAAJ,IAAA;QACA,IAAAA,IAAA,CAAAG,EAAA,KAAAA,EAAA,EAAAH,IAAA,CAAAK,IAAA,IAAAL,IAAA,CAAAK,IAAA;MACA;IACA;IACAC,YAAAH,EAAA;MACAI,OAAA,CAAAC,GAAA,CAAAL,EAAA;MACA,KAAAV,IAAA,QAAAA,IAAA,CAAAgB,MAAA,CAAAT,IAAA;QACA,OAAAA,IAAA,CAAAG,EAAA,KAAAA,EAAA;